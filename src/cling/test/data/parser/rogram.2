int mod(int x, int y)
{
    int z;
    z = x - x/y*y;
	printf(" z = ",z);
    return(z);
}
int gcd(int a, int b)
{
    if (b==0) return(0);
    else if (mod(a,b)==0)
        return(b);
    else return(gcd(b, mod(a,b)));
}
void main(){
	int a ,b,r;
	a = 15;
	b = 24;
	r = gcd(a,b);
	printf(" ",r);
}

